@page
@model WebAppUndertheHood.Pages.HRManagerModel
@{
}
<div>
    <h1>HR Manager</h1>
    <br />
</div>


<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <th>Temp C</th>
            <th>Temp F</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.whateverForecastItems)
        {
            <tr>
                <td>@item.Date.ToLongDateString()</td>
                <td>@item.TemperatureC</td>
                <td>@item.TemperatureF</td>
                <td>@item.Summary</td>
            </tr>
        }
    </tbody>
</table>
